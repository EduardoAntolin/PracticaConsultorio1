<UserControl x:Class="PracticaConsultorio.ElementoConsultaControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PracticaConsultorio"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid Background="#FFF5CFCF">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" > </ColumnDefinition>
            <ColumnDefinition Width="*" ></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="lblSintomas" Text="Sintomas" Grid.Column="0" Grid.Row="0" />
        <TextBlock x:Name="lblDiagnostico" Text="Diagnostico" Grid.Column="1" Grid.Row="0" />
        <TextBlock x:Name="lblReceta" Text="Receta" Grid.Column="0" Grid.Row="1" />
        <TextBlock x:Name="lblFecha" Text="Fecha" Grid.Column="1" Grid.Row="1"/>
    </Grid>
</UserControl>
